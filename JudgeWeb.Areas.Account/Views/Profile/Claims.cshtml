@{
    ViewData["Title"] = "Claims";
}

<h4>Claims</h4>
<table class="table table-sm mt-3">
    <tr>
        <th>type</th>
        <th>value</th>
    </tr>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>
